{
    "name": "DECP datasette",
    "id": "decp-datasette",
    "packaging_format": 1,
    "description": {
        "en": "DECP datasette",
        "fr": "DECP datasette"
    },
    "version": "1.0~ynh1",
    "url": "https://decp.info",
    "license": "free",
    "maintainer": {
        "name": "Colin Maudry",
        "email": "colin@maudry.com",
        "url": "http://example.com"
    },
    "requirements": {
        "yunohost": ">= 4.1.3"
    },
    "multi_instance": false,
    "services": [
        "nginx",
        "python3-pip",
        "python3"
    ],
    "arguments": {
        "install" : [
            {
                "name": "domain",
                "type": "domain",
                "example": "example.com"
            },
            {
                "name": "path",
                "type": "path",
                "example": "/example",
                "default": "/"
            },
            {
                "name": "is_public",
                "type": "boolean",
                "default": true
            }
        ]
    }
}
